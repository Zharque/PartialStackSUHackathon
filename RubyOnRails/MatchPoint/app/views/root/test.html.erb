<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
   <div class="field">
     <%= f.label :gender %><br />
     <%= f.select :gender, [['Male', 'Male'], ['Female', 'Female'], ['Other', 'Other']] %>
   </div>

   <div class="field">
     <%= f.label :birthdate %><br />
     <%= f.date_field :birthdate %>
   </div>

   <div class="field">
     <%= f.label :field %><br/>
     <%= f.text_field :field %>
   </div>

   <div class="field">
     <%= f.label :account_type %><br />
     <%= f.select :account_type, [['Individual', 'Individual'], ['Business', 'Business']] %>
   </div>

   <% end %>